{
    "model": {
        "model_name": "TinyLlama/TinyLlama-1.1B-Chat-v1.0",
        "model_dtype": "bf16",
        "model_method": "hybird3",
        "model_structure": "llama",
        "save_ckp": "ckp/hybird3-2.pth",
        "load_ckp": null,
        "config": "config/hybird3-32.json",
        "device_map": {
            "": 0
        }
    },

    "train": {
        "train_iters": 10000,
        "max_lr": 3e-6,
        "warmup": 0.01,
        "beta1": 0.9,
        "beta2": 0.999,
        "weight_decay": 0,
        "corpus": [
            {
                "name": "beacons sampled.4096",
                "partition": 1,
                "truncation": 2048
            }
        ],
        "accum_grad": 1,
        "clip_grad": 1.0,

        "save": 1000,
        "eval": 1000,
        "tasks": []
    }
}